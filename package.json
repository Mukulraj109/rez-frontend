{
  "name": "frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=16384 expo start",
    "start:clear": "cross-env NODE_OPTIONS=--max-old-space-size=16384 expo start --clear",
    "start:managed": "node --expose-gc --max-old-space-size=16384 scripts/start-dev.js --clear",
    "start:lowmem": "cross-env NODE_OPTIONS=--max-old-space-size=8192 expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "clear-cache": "node ./scripts/clear-cache.js",
    "android": "cross-env NODE_OPTIONS=--max-old-space-size=16384 expo start --android",
    "ios": "cross-env NODE_OPTIONS=--max-old-space-size=16384 expo start --ios",
    "web": "cross-env NODE_OPTIONS=--max-old-space-size=16384 expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:build:android": "detox build --configuration android.emu.debug",
    "verify:production": "ts-node scripts/verify-production-readiness.ts",
    "check:backend": "node scripts/check-backend.js",
    "build:render": "npx expo export --platform web",
    "serve:render": "npx serve dist",
    "serve:web": "npx serve dist"
  },
  "dependencies": {
